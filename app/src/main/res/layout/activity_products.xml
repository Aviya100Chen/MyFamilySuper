<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".products">


    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="0dp"
        android:layout_height="168dp"
        android:background="#5DE1E6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/main_icon" />

    <Button
        android:id="@+id/fruit_vege"
        android:layout_width="671dp"
        android:layout_height="95dp"
        android:layout_weight="0.7"
        android:text="ירקות ופירות"
        android:textColorLink="#9C27B0"
        android:textSize="60sp"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="49dp" />

    <Button
        android:id="@+id/milk_eggs"
        android:layout_width="670dp"
        android:layout_height="105dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="96dp"
        android:layout_marginEnd="56dp"
        android:layout_weight="0.7"
        android:text="מוצרי חלב וביצים"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/milk_eggs_pic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/meat_fish"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/meat_fish"
        android:layout_width="677dp"
        android:layout_height="92dp"
        android:layout_weight="0.7"
        android:text="בשר ודגים"
        android:textSize="60sp"
        tools:layout_editor_absoluteX="70dp"
        tools:layout_editor_absoluteY="740dp" />

    <ImageView
        android:id="@+id/fruit_vege_pic"
        android:layout_width="189dp"
        android:layout_height="142dp"
        android:layout_weight="0.3"
        app:srcCompat="@drawable/fruit_vegetables"
        tools:layout_editor_absoluteX="772dp"
        tools:layout_editor_absoluteY="1462dp" />

    <ImageView
        android:id="@+id/meat_fish_pic"
        android:layout_width="172dp"
        android:layout_height="150dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="31dp"
        android:layout_weight="0.3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/meat_fish"
        app:layout_constraintTop_toBottomOf="@+id/fruit_vege_pic"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/meat_fish" />

    <ImageView
        android:id="@+id/milk_eggs_pic"
        android:layout_width="194dp"
        android:layout_height="134dp"
        android:layout_weight="0.3"
        app:srcCompat="@drawable/milk_eggs"
        tools:layout_editor_absoluteX="523dp"
        tools:layout_editor_absoluteY="1231dp" />

    <Button
        android:id="@+id/breads_legumes"
        android:layout_width="674dp"
        android:layout_height="106dp"
        android:layout_marginEnd="56dp"
        android:layout_weight="0.7"
        android:text="לחם וקטניות"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/drinks_pic"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/milk_eggs"
        app:layout_constraintVertical_bias="0.04" />

    <ImageView
        android:id="@+id/breads_legumes_pic"
        android:layout_width="211dp"
        android:layout_height="142dp"
        android:layout_weight="0.3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.98"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.506"
        app:srcCompat="@drawable/bread_legums" />

    <Button
        android:id="@+id/drinks"
        android:layout_width="683dp"
        android:layout_height="103dp"
        android:layout_weight="0.7"
        android:text="משקאות"
        android:textSize="60sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.093"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.664" />

    <Button
        android:id="@+id/candy_snacks"
        android:layout_width="680dp"
        android:layout_height="107dp"
        android:layout_weight="0.7"
        android:text="חטיפים וממתקים"
        android:textSize="60sp"
        tools:layout_editor_absoluteX="281dp"
        tools:layout_editor_absoluteY="1497dp" />

    <ImageView
        android:id="@+id/drinks_pic"
        android:layout_width="216dp"
        android:layout_height="123dp"
        android:layout_weight="0.3"
        app:srcCompat="@drawable/drinks"
        tools:layout_editor_absoluteX="702dp"
        tools:layout_editor_absoluteY="985dp" />

    <ImageView
        android:id="@+id/candy_snacks_pic"
        android:layout_width="180dp"
        android:layout_height="144dp"
        android:layout_weight="0.3"
        app:srcCompat="@drawable/candy_snacks"
        tools:layout_editor_absoluteX="766dp"
        tools:layout_editor_absoluteY="1221dp" />

</androidx.constraintlayout.widget.ConstraintLayout>